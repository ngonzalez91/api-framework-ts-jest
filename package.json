{
    "name": "api-framework",
    "version": "1.0.0",
    "description": "API Test Automation framework using TS, Axios and Jest",
    "main": "index.js",
    "scripts": {
        "lint": "npx eslint",
        "test": "npx eslint && jest --runInBand",
        "test:deleteBooking": "npx eslint && jest -t=DeleteBooking --runInBand",
        "test:addBooking": "npx eslint && jest -t=AddBooking --runInBand",
        "test:smoke": "npx eslint && jest -t=Smoke --runInBand",
        "test:regression": "npx eslint && jest -t=@Regression --runInBand",
        "prettier:fix": "prettier --write ."
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/jest": "^27.4.1",
        "@typescript-eslint/eslint-plugin": "^6.6.0",
        "axios-logger": "^2.6.0",
        "eslint": "^8.48.0",
        "eslint-config-standard-with-typescript": "^39.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-n": "^16.0.2",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^27.5.1",
        "jest-extended": "^2.0.0",
        "prettier": "3.0.3",
        "ts-jest": "^27.1.3",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@types/node": "^17.0.21",
        "axios": "^0.26.0",
        "dotenv": "^16.0.0"
    }
}
